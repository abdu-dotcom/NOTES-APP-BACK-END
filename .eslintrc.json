{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": [2, "tab"],
        "no-tabs": 0,
        "no-new-object": "error",
        "no-console": "off",
        "linebreak-style": ["error", "windows"]
    }
}
